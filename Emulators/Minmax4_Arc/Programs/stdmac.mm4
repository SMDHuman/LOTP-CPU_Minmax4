; Standart Macros
; Version 1.2

<push_regs psh r0, psh r1 psh, r2>
<pop_regs pop r2, pop r1 pop, r0>
<halt 
  mov r0 0
  sub r0 1
  mov pc r0 
>
<jump <name> psh r2, mov r2 name, mov pc r2>
<land <name> name: pop r2>

<call <sub_name>
  mov r2 _HERE_
  add r2 8
  psh r2
  mov r2 sub_name
  mov pc r2
  ; Returns here
>
<return 
  pop pc
>
